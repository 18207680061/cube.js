{
  "name": "cubejs-cli",
  "description": "Cube.js Command Line Interface",
  "author": "Cube Dev, Inc.",
  "version": "0.20.11",
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-js/cube.js.git",
    "directory": "packages/cubejs-cli"
  },
  "engines": {
    "node": ">=8.11.1"
  },
  "bin": {
    "cubejs": "bin/run"
  },
  "scripts": {
    "tsc": "tsc",
    "watch": "tsc -w",
    "test": "npm run unit",
    "unit": "jest",
    "lint": "eslint -c ../../.eslintrc.js *.js",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf dist && tsc -b && oclif-dev manifest"
  },
  "files": [
    "bin/*",
    "dist/*",
    "/yarn.lock",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.3",
    "@oclif/plugin-help": "^3.2.0",
    "chalk": "^2.4.2",
    "cli-progress": "^3.8.2",
    "cross-spawn": "^7.0.1",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.1.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "node-machine-id": "^1.1.10",
    "request": "^2.88.2",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.7",
    "@types/fs-extra": "^9.0.1",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.11.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^5.2.1",
    "husky": "^4.2.3",
    "jest": "^24.8.0",
    "mocha": "^8.0.1",
    "should": "^13.2.3",
    "typescript": "^4.0.3"
  },
  "license": "Apache-2.0",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "cubejs",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
